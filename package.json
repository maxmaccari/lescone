{
  "name": "lescone",
  "version": "1.0.0",
  "description": "A example project created on CSS com SASS course by Origamid.",
  "main": "index.html",
  "scripts": {
    "start": "npm run dev",
    "build": "gulp build",
    "lint": "gulp lint",
    "dev": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmaccari/lescone.git"
  },
  "keywords": [
    "example",
    "typography",
    "sass",
    "website"
  ],
  "author": "Maxsuel Maccari <maxmaccari@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxmaccari/lescone/issues"
  },
  "homepage": "https://github.com/maxmaccari/lescone#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "browser-sync": "^2.26.5",
    "gulp": "^4.0.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "sass": "^1.19.0",
    "tar": ">=4.4.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true
    },
    "globals": {
      "document": "readonly",
      "window": "readonly"
    },
    "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "single"],
      "indent": ["error", 2]
    }
  },
  "eslintIgnore": [
    "gulpfile.js"
  ]
}
